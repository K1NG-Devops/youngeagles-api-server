{
  "name": "young-eagles-preschool-platform",
  "version": "2.0.0",
  "description": "Young Eagles Preschool Platform - Secure, scalable API for multi-tenant preschool management",
  "main": "src/index.production.js",
  "type": "module",
  "scripts": {
    "start": "NODE_ENV=production node src/index.production.js",
    "start:prod": "NODE_ENV=production node src/index.production.js",
    "start:dev": "NODE_ENV=development node --watch src/index.js",
    "start:local": "NODE_ENV=development node src/index-local.js",
    "railway": "NODE_ENV=production node src/index.production.js",
    "dev": "NODE_ENV=development node --watch src/index.js",
    "test": "NODE_ENV=test jest --detectOpenHandles",
    "test:watch": "NODE_ENV=test jest --watch --detectOpenHandles",
    "lint": "eslint src/ --ext .js,.jsx",
    "lint:fix": "eslint src/ --ext .js,.jsx --fix",
    "logs": "tail -f logs/combined.log",
    "logs:error": "tail -f logs/error.log",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "check-config": "node scripts/check-config.js"
  },
  "keywords": [
    "young-eagles",
    "preschool",
    "platform",
    "api",
    "pwa",
    "education",
    "homework",
    "multi-tenant",
    "scalable"
  ],
  "author": "Young Eagles Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.10.0",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5",
    "nodemailer": "^6.9.7",
    "socket.io": "^4.7.4",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/youngeagles/preschool-platform.git"
  },
  "bugs": {
    "url": "https://github.com/youngeagles/preschool-platform/issues"
  },
  "homepage": "https://youngeagles.com"
}
